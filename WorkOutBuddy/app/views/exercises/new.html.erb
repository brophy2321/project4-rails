<%= form_for :exercise, url: exercises_path do |f| %>
  <p>
    <%= f.label :workout %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :photoUrl %><br>
    <%= f.text_area :photoUrl %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %><br>
  </p>
  <p>
    <%= f.label :video%><br>
    <%= f.text_area :description %><br>
  </p>


  <p>
    <%= f.submit %>
  </p>
<% end %>
